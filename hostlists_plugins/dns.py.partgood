#!/usr/bin/env python
""" hostlists plugin to get hosts from dns """
import dns.resolver

def name():
  return 'dns'

def expand(value):
  tmplist=[]
  adresses=[]
  try:
    answers = list(dns.resolver.query(value))
  except dns.resolver.NoAnswer:
    answers=[]
  for rdata in answers:
    try:
      tmplist.append(rdata.address)
    except AttributeError:
      tmplist.append(rdata.qname)
  print tmplist
  return tmplist
  